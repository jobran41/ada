{
  "name": "react-redux-firebase-authentication",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "firebase": "^5.0.4",
    "prop-types": "^15.6.1",
    "react": "^16.4.0",
    "react-dom": "^16.4.0",
    "react-hoc-mutate": "^1.0.3",
    "react-hoc-query": "^1.2.4",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.2.2",
    "react-scripts": "1.1.4",
    "reactutils": "^2.3.7",
    "recompose": "^0.27.1",
    "redux": "^4.0.0",
    "history": "^4.7.2",
    "redux-logger": "^3.0.6",
    "react-router-redux": "^5.0.0-alpha.9",
    "redux-promise-middleware": "^4.4.1",
    "redux-thunk": "^2.2.0",
    "redux-actions": "^2.2.1",
    "react-md": "^1.2.8"
  },
  "scripts": {
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "build:js": "react-scripts build && node src/utils/serviceWorkerIgnoreLegacyUri.js",
    "build": "npm-run-all build-css build:js && npm run delete-maps",
    "test": "npm run build-css && react-scripts test --env=jsdom",
    "test:js": "react-scripts test --env=jsdom",
    "test:ci": "npm run test -- --ci --coverage --watch=0 ",
    "eject": "react-scripts eject",
    "build-css": "node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/ --watch --recursive",
    "flow": "flow",
    "lint": "eslint ./src",
    "precommit": "lint-staged",
    "prepush": "npm-run-all lint test:ci"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "eslintIgnore": [
    "flow-typed",
    "build",
    "coverage",
    "public"
  ],
  "devDependencies": {
    "babel-eslint": "^8.2.3",
    "cookies-js": "^1.2.3",
    "eslint": "^4.19.1",
    "eslint-config-prettier": "^2.4.0",
    "eslint-plugin-flowtype": "^2.49.3",
    "eslint-plugin-prettier": "^2.2.0",
    "flow-bin": "^0.54.1",
    "lint-staged": "^4.1.3",
    "node-sass-chokidar": "^0.0.3",
    "npm-run-all": "^4.1.1",
    "prettier": "^1.6.1",
    "react-scripts": "1.0.13"
  }
}
